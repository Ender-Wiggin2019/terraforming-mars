  "scripts": {
    "start": "node build/src/server/server.js",
    "starttest": "node build/src/server/server_test.js",
    "start:prof": "node --prof build/src/server/server.js",
    "build": "npm run make:static && npm run build:server && npm run build:client",
    "build:client": "npm run make:cards && cross-env NODE_ENV=production webpack",
    "build:clientAll": "npm run make:cards && npm run test:client:HTML && cross-env NODE_ENV=production webpack",
    "build:server": "tsc --build src/tsconfig.json",
    "build:tests": "tsc --build tests/tsconfig.json",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:client": "vti diagnostics",
    "lint:server": "eslint --cache src tests --ext ts --ext vue",
    "lint:fix": "eslint --fix src tests --ext ts --ext vue",
    "make:cards": "ts-node src/server/tools/export_card_rendering.ts",
    "make:css": "lessc src/styles/common.less build/styles.css && npx tailwindcss -i ./src/styles/tailwindcss.css -o ./build/tailwindcss.css",
    "make:json": "node make_static_json.js",
    "make:static": "npm run make:css && npm run make:json",
    "test": "npm run test:server && npm run test:client",
    "test:server": "ts-mocha -p tests/tsconfig.json --reporter-option maxDiffSize=256 -r tests/utils/setup.ts 'tests/*.spec.ts' \"tests/!(client|integration)/**/*.spec.ts\"",
    "test:one": "ts-mocha  --timeout 1000000 -p tests/tsconfig.json --reporter-option maxDiffSize=256  -r tests/utils/setup.ts  \"tests/!(client|integration)/**/CloneTechnology.spec.ts\" ",
    "test:integration": "ts-mocha --timeout 1000000 -p tests/tsconfig.json \"tests/integration/**/*.spec.ts\"",
    "test:postgresql": "ts-mocha -p tests/tsconfig.json \"tests/integration/PostgreSQL.spec.ts\"",
    "test:client": "cross-env NODE_ENV=development mochapack --require tests/client/components/setup.ts \"tests/client/**/*.spec.ts\"",
    "test:client:watch": "cross-env NODE_ENV=development mochapack --watch --require tests/client/components/setup.ts \"tests/client/**/*.spec.ts\"",
    "test:client:HTML": "cross-env   mochapack --timeout 60000    --require tests/client/components/setup.ts   tests/client/components/Card_HTML.spec.ts",
    "cover": "nyc ts-mocha -p tests/tsconfig.json -r tests/utils/setup.ts \"tests/*.spec.ts\" \"tests/!(client)/**/*.spec.ts\"",
    "dev:client": "cross-env NODE_ENV=development webpack --watch",
    "dev:server": "cross-env NODE_ENV=development tsnd src/server/server.ts",
    "tsc-w": "tsc --build -w src/tsconfig.json",
    "tsc-wt": "tsc --build -w src/tsconfig.json  tests/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terraforming-mars/terraforming-mars.git"
